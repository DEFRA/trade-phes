{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "type":"object",
  "properties":{
    "applicationId":{
      "type":"integer"
    },
    "applicantId":{
      "type":"string",
      "pattern":"[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
    },
    "cancellationDateTime":{
      "type":"string",
      "format":"date-time"
    }
  },
  "required":[
    "applicationId",
    "applicantId",
    "cancellationDateTime"
  ]
}